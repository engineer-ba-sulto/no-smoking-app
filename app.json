{
  "expo": {
    "name": "no-smoking-app",
    "slug": "no-smoking-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.engineer-ba-sulto.no-smoking-app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-sqlite", "expo-dev-client"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "root": "./src/app"
      },
      "eas": {
        "projectId": "83cf026b-56b5-4e7e-916e-5ad8057581c1"
      }
    }
  }
}
